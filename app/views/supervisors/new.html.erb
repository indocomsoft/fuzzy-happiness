<%= form_for @supervisor do |f| %>
<table>
  <tr>
    <td>Full Name:</td>
    <td><%= f.text_field :fullname %></td>
  </tr>
  <tr>
    <td>Phone number:</td>
    <td><%= f.text_field :phone_number %></td>
  </tr>
  <tr>
    <td>Email address:</td>
    <td><%= f.text_field :email %></td>
  </tr>
</table>
<%= f.submit "Submit" %>
<% end %>